<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0-Windows</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<Version>3.5.0</Version>
		<FileVersion>3.5.0</FileVersion>
		<AssemblyVersion>3.5.0</AssemblyVersion>
		<Platforms>AnyCPU;x64</Platforms>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Services\VirtualPrinter.Handler.Zpl.json" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="Services\VirtualPrinter.Handler.Zpl.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Diamond.Core.Repository" Version="9.0.1" />
		<PackageReference Include="Diamond.Core.Repository.Abstractions" Version="9.0.1" />
		<PackageReference Include="Diamond.Core.System.Text" Version="9.0.1" />
		<PackageReference Include="Diamond.Core.Wpf" Version="9.0.1" />
		<PackageReference Include="Diamond.Core.Extensions.Hosting" Version="9.0.1" />
		<PackageReference Include="UnitsNet" Version="5.74.0" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\VirtualPrinter.Handler.Abstractions\VirtualPrinter.Handler.Abstractions.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Content Update="$(UserProfile)\.nuget\packages\diamond.core.repository.abstractions\9.0.1\contentFiles\any\net9.0\Services\Diamond.Core.Repository.Abstractions.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <Content Update="$(UserProfile)\.nuget\packages\diamond.core.repository\9.0.1\contentFiles\any\net9.0\Services\Diamond.Core.Repository.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>

	<ItemGroup>
		<None Include="..\.editorconfig" Link=".editorconfig" />
	</ItemGroup>
</Project>
